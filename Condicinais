<html>
  <head>
    <title>Curso Javascript</title>
  </head>

  <body>
    <script>
      function retornasexo(sexo) {
        //M, F
        //Masculino, Feminino
        if (sexo === "M") {
          return "Masculino";
        } else if (sexo === "F") {
          return "Feminino";
        } else {
          return "Outro";
        }
      }
      var resultado = retornasexo("asdioasdj");
      console.log(resultado);
    </script>
  </body>
</html>

